<Window x:Class="Hiker_Editor.Views.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hiker_Editor.Views"
        mc:Ignorable="d"
        Title="Setting" Height="380" Width="600" Icon="/Hiker-Editor;component/Resources/Images/setting.png" ShowInTaskbar="False">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition Width="59*"/>
        </Grid.ColumnDefinitions>
        <ListBox>
            <ListBoxItem Background="White" Content="Main" Foreground="Black" IsSelected="True" VerticalAlignment="Top"/>
            <ListBoxItem Content="Project"/>
            <ListBoxItem Content="Code Editor"/>
            <ListBoxItem Content="Other"/>
        </ListBox>

    </Grid>
</Window>
